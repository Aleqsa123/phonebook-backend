{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "consistent-return": "off",
        "no-unused-vars": "off",
        "no-underscore-dangle": "off",
        "no-param-reassign": "off",
        "import/extensions": [
            "error",
            "ignorePackages",
            {
              "jsx": "never",
              "ts": "never",
              "tsx": "never"
            }
         ]
    }
}
